<?xml version="1.0" encoding="UTF-8"?>

<ipxact:component xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ipxact="http://www.accellera.org/XMLSchema/IPXACT/1685-2014"
    xsi:schemaLocation="http://www.accellera.org/XMLSchema/IPXACT/1685-2014 http://www.accellera.org/XMLSchema/IPXACT/1685-2014/index.xsd">
    <ipxact:vendor>Autotalks</ipxact:vendor>
    <ipxact:library>AutoTalksLib</ipxact:library>
    <ipxact:name>plug_rgf</ipxact:name>
    <ipxact:version>0.0</ipxact:version>
    <ipxact:description>Auto Regfiles</ipxact:description>
    <ipxact:memoryMaps>
        <ipxact:memoryMap>
            <ipxact:name>MAP</ipxact:name>
            <ipxact:isPresent>true</ipxact:isPresent>
            <ipxact:addressUnitBits>8</ipxact:addressUnitBits>
            <ipxact:addressBlock>
                <ipxact:name>BLOCK</ipxact:name>
                <ipxact:isPresent>true</ipxact:isPresent>
                <ipxact:baseAddress>'h0</ipxact:baseAddress>
                <ipxact:range>'h18</ipxact:range>
                <ipxact:width>32</ipxact:width>

               <ipxact:register>
                    <ipxact:name>control_0</ipxact:name>
                    <ipxact:isPresent>true</ipxact:isPresent>
                    <ipxact:addressOffset>'h0</ipxact:addressOffset>
                    <ipxact:description>"this is very important register"</ipxact:description>
                    <ipxact:size>32</ipxact:size>
                    <ipxact:volatile>false</ipxact:volatile>
                    <ipxact:access>read-write</ipxact:access>
                    <ipxact:field>
                        <ipxact:name>control_0</ipxact:name>
                        <ipxact:isPresent>true</ipxact:isPresent>
                        <ipxact:bitOffset>0</ipxact:bitOffset>
                        <ipxact:bitWidth>'h20</ipxact:bitWidth>
                        <ipxact:volatile>false</ipxact:volatile>
                        <ipxact:resets> <ipxact:reset> <ipxact:value>0</ipxact:value> </ipxact:reset> </ipxact:resets>
                        <ipxact:vendorExtensions>
                            <userLogic>false</userLogic>
                        </ipxact:vendorExtensions>
                    </ipxact:field>
                    <ipxact:vendorExtensions>
                    </ipxact:vendorExtensions>
                </ipxact:register>

               <ipxact:register>
                    <ipxact:name>control_1</ipxact:name>
                    <ipxact:isPresent>true</ipxact:isPresent>
                    <ipxact:addressOffset>'h4</ipxact:addressOffset>
                    <ipxact:description>"this is very important register"</ipxact:description>
                    <ipxact:size>32</ipxact:size>
                    <ipxact:volatile>false</ipxact:volatile>
                    <ipxact:access>read-write</ipxact:access>
                    <ipxact:field>
                        <ipxact:name>control_1</ipxact:name>
                        <ipxact:isPresent>true</ipxact:isPresent>
                        <ipxact:bitOffset>0</ipxact:bitOffset>
                        <ipxact:bitWidth>'h20</ipxact:bitWidth>
                        <ipxact:volatile>false</ipxact:volatile>
                        <ipxact:resets> <ipxact:reset> <ipxact:value>0</ipxact:value> </ipxact:reset> </ipxact:resets>
                        <ipxact:vendorExtensions>
                            <userLogic>false</userLogic>
                        </ipxact:vendorExtensions>
                    </ipxact:field>
                    <ipxact:vendorExtensions>
                    </ipxact:vendorExtensions>
                </ipxact:register>

               <ipxact:register>
                    <ipxact:name>control_2</ipxact:name>
                    <ipxact:isPresent>true</ipxact:isPresent>
                    <ipxact:addressOffset>'h8</ipxact:addressOffset>
                    <ipxact:description>"this is very important register"</ipxact:description>
                    <ipxact:size>32</ipxact:size>
                    <ipxact:volatile>false</ipxact:volatile>
                    <ipxact:access>read-write</ipxact:access>
                    <ipxact:field>
                        <ipxact:name>control_2</ipxact:name>
                        <ipxact:isPresent>true</ipxact:isPresent>
                        <ipxact:bitOffset>0</ipxact:bitOffset>
                        <ipxact:bitWidth>'h8</ipxact:bitWidth>
                        <ipxact:volatile>false</ipxact:volatile>
                        <ipxact:resets> <ipxact:reset> <ipxact:value>0</ipxact:value> </ipxact:reset> </ipxact:resets>
                        <ipxact:vendorExtensions>
                            <userLogic>false</userLogic>
                        </ipxact:vendorExtensions>
                    </ipxact:field>
                    <ipxact:vendorExtensions>
                    </ipxact:vendorExtensions>
                </ipxact:register>

               <ipxact:register>
                    <ipxact:name>status</ipxact:name>
                    <ipxact:isPresent>true</ipxact:isPresent>
                    <ipxact:addressOffset>'hc</ipxact:addressOffset>
                    <ipxact:description>"read only register"</ipxact:description>
                    <ipxact:size>32</ipxact:size>
                    <ipxact:volatile>true</ipxact:volatile>
                    <ipxact:access>read-only</ipxact:access>
                    <ipxact:field>
                        <ipxact:name>status</ipxact:name>
                        <ipxact:isPresent>true</ipxact:isPresent>
                        <ipxact:bitOffset>0</ipxact:bitOffset>
                        <ipxact:bitWidth>'h20</ipxact:bitWidth>
                        <ipxact:volatile>true</ipxact:volatile>
                        <ipxact:resets> <ipxact:reset> <ipxact:value>0</ipxact:value> </ipxact:reset> </ipxact:resets>
                        <ipxact:vendorExtensions>
                            <userLogic>true</userLogic>
                        </ipxact:vendorExtensions>
                    </ipxact:field>
                    <ipxact:vendorExtensions>
                    </ipxact:vendorExtensions>
                </ipxact:register>

               <ipxact:register>
                    <ipxact:name>control2</ipxact:name>
                    <ipxact:isPresent>true</ipxact:isPresent>
                    <ipxact:addressOffset>'h10</ipxact:addressOffset>
                    <ipxact:description>"fielded register, no need for width here"</ipxact:description>
                    <ipxact:size>32</ipxact:size>
                    <ipxact:volatile>false</ipxact:volatile>
                    <ipxact:access>read-write</ipxact:access>
                    
                    <ipxact:field>
                        <ipxact:name>rate</ipxact:name>
                        <ipxact:isPresent>true</ipxact:isPresent>
                        <ipxact:bitOffset>0</ipxact:bitOffset>
                        <ipxact:bitWidth>6</ipxact:bitWidth>
                        <ipxact:volatile>false</ipxact:volatile>
                        <ipxact:resets> <ipxact:reset> <ipxact:value>0</ipxact:value> </ipxact:reset> </ipxact:resets>
                        <ipxact:vendorExtensions>
                            <userLogic>false</userLogic>
                        </ipxact:vendorExtensions>
                        <ipxact:description>"baud rate"</ipxact:description>
                    </ipxact:field>

                    <ipxact:field>
                        <ipxact:name>dsc</ipxact:name>
                        <ipxact:isPresent>true</ipxact:isPresent>
                        <ipxact:bitOffset>6</ipxact:bitOffset>
                        <ipxact:bitWidth>1</ipxact:bitWidth>
                        <ipxact:volatile>false</ipxact:volatile>
                        <ipxact:resets> <ipxact:reset> <ipxact:value>0</ipxact:value> </ipxact:reset> </ipxact:resets>
                        <ipxact:vendorExtensions>
                            <userLogic>false</userLogic>
                        </ipxact:vendorExtensions>
                        
                    </ipxact:field>

                    <ipxact:field>
                        <ipxact:name>bdd</ipxact:name>
                        <ipxact:isPresent>true</ipxact:isPresent>
                        <ipxact:bitOffset>7</ipxact:bitOffset>
                        <ipxact:bitWidth>4</ipxact:bitWidth>
                        <ipxact:volatile>false</ipxact:volatile>
                        <ipxact:resets> <ipxact:reset> <ipxact:value>0</ipxact:value> </ipxact:reset> </ipxact:resets>
                        <ipxact:vendorExtensions>
                            <userLogic>false</userLogic>
                        </ipxact:vendorExtensions>
                        
                    </ipxact:field>

                    <ipxact:vendorExtensions>
                    </ipxact:vendorExtensions>
                </ipxact:register>
 



            </ipxact:addressBlock>
        </ipxact:memoryMap>
    </ipxact:memoryMaps>


</ipxact:component>
